<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Secuencia para Órdenes de Reparación -->
        <record id="seq_mobile_repair_order" model="ir.sequence">
            <field name="name">Órdenes de Reparación Móvil</field>
            <field name="code">mobile.repair.order</field>
            <field name="prefix">REP/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
